{
    "manifest_version": 2,
    "version": "0.1",
    "name": "Auto Cookie Optout",
    "content_scripts": [
        {
            "matches": ["*://consent.youtube.com/*"],
            "js": ["scripts/youtube.js"]
        },
        {
            "matches": [
                "*://www.google.com/*",
                "*://www.google.be/*",
                "*://www.google.co.uk/*",
                "*://www.google.fr/*",
                "*://www.google.nl/*",
                "*://consent.google.com/*",
                "*://consent.google.be/*",
                "*://consent.google.co.uk/*",
                "*://consent.google.fr/*",
                "*://consent.google.nl/*"
            ],
            "js": ["scripts/google.js"]
        },
        {
            "matches": ["*://consent.yahoo.com/*"],
            "js": ["scripts/yahoo.js"]
        }
    ]
}